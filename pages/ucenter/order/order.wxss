.btn_goods{
  position: absolute;
  top: 60rpx;
  right: 8%;
  width: 150rpx;
  height: 60rpx;
  /* background: #67e956; */
  background-color: rgb(38, 132, 240);
  z-index: 99999;
}

.btn_goods::after{
  border: 0ch;
}
.buy{
  position: absolute;
