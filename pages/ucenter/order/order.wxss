page {
  height: 100%;
  width: 100%;
  background-color: white;
}
.content{
  position: absolute;
  top:0rpx;
  left:0%;
  width:100%;
}
.button_item{
  width:95%;
  margin: 20rpx auto;
  padding: 30rpx;
  background: #fff;
  box-shadow: 2px 2px 3px #cfcece;
  border-radius: 15rpx;
  display: flex;
  position: relative;
}
.goods{
  width: 80%;
}
.goods::after{
  border:0px;
}
.goods_head{
  position: absolute;
  top:0rpx;
  left:0%;
  width:100%;
  height: 65rpx;
}
.logo{
  position: absolute;
  top:12.5rpx;
  left:5%;
  width:40rpx;
  height: 40rpx;
}
.divLine{
  position: absolute;
  background: #E0E3DA;
  top:65rpx;
  width: 91%;
  height:3rpx;
}
.goods_logo{
  width:75px;
  height: 75px;
  margin: auto;
}
.name{
  position: absolute;
  top:40rpx;
  left:25%;
  width:39%;
  font-size: 30rpx;
  text-align: left;
}
.goods_name{
  width:90%;
  font-size: 30rpx;
  text-align: left;
  margin: 10rpx 0 0 20rpx;
  display:-webkit-box;
  -webkit-line-clamp:1;
  overflow:hidden;
  text-overflow:ellipsis;
  -webkit-box-orient:vertical;
  word-break:break-all;
}
.table_row{
  position: absolute;
  top:120rpx;
  left:25%;
  width:39%;
  font-size: 25rpx;
  text-align: left;
}
.introduce{
  position: absolute;
  top:160rpx;
  left:25%;
  width:100%;
  text-align: left;
  font-size: 25rpx;
  /* height: 55rpx;
  color:rgb(151, 150, 150);
