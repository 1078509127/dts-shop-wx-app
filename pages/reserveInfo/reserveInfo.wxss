  .search {
    height: 100rpx;
    width: 100%;
    padding: 0 30rpx;
    background: #fff;
    display: flex;
    align-items: center;
  }
  .search .input {
    width: 600rpx;
    height: 70rpx;
    border-radius: 20rpx;
    display: flex;
    padding-top: 18rpx;
    padding-left: 20rpx;
  }
  .search .icon {
    background: url('data:image/png;base64,') center no-repeat;
    background-size: 100%;
    width: 34rpx;
    height: 34rpx;
    margin-top: 6rpx;
  }
  .search .txt {
    height: 42rpx;
    line-height: 42rpx;
    padding-left: 10rpx;
    width: 100%;
  }
.search .btn{
  width: 140rpx;
